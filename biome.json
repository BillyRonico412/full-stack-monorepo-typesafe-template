{
	"$schema": "https://biomejs.dev/schemas/1.7.3/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"all": true,
			"style": {
				"all": true,
				"useNamingConvention": "off"
			}
		},
		"ignore": [
			"./front/src/routeTree.gen.ts"
		]
	},
	"javascript": {
		"formatter": {
			"semicolons": "asNeeded"
		}
	}
}